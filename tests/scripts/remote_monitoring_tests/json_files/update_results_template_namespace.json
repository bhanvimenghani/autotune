[
  {
    "version": "{{version}}",
    "experiment_name": "{{experiment_name}}",
    "interval_start_time": "{{interval_start_time}}",
    "interval_end_time": "{{interval_end_time}}",
    "kubernetes_objects": [
      {
        "namespaces":
        {
          "namespace": "{{namespace}}",
          "metrics": [
            {
              "name": "{{namespaceCpuRequest_name}}",
              "results": {
                "aggregation_info": {
                  "sum": {{namespaceCpuRequest_sum}},
                  "format": "{{namespaceCpuRequest_format}}"
                }
              }
            },
            {
              "name": "{{namespaceCpuLimit_name}}",
              "results": {
                "aggregation_info": {
                  "sum": {{namespaceCpuLimit_sum}},
                  "format": "{{namespaceCpuLimit_format}}"
                }
              }
            },
            {
              "name": "{{namespaceCpuUsage_name}}",
              "results": {
                "aggregation_info": {
                  "min": {{namespaceCpuUsage_min}},
                  "max": {{namespaceCpuUsage_max}},
                  "avg": {{namespaceCpuUsage_avg}},
                  "format": "{{namespaceCpuUsage_format}}"
                }
              }
            },
            {
              "name": "{{namespaceCpuThrottle_name}}",
              "results": {
                "aggregation_info": {
                  "min": {{namespaceCpuThrottle_min}},
                  "max": {{namespaceCpuThrottle_max}},
                  "avg": {{namespaceCpuThrottle_avg}},
                  "format": "{{namespaceCpuThrottle_format}}"
                }
              }
            },
            {
              "name": "{{namespaceMemoryRequest_name}}",
              "results": {
                "aggregation_info": {
                  "sum": {{namespaceMemoryRequest_sum}},
                  "format": "{{namespaceMemoryRequest_format}}"
                }
              }
            },
            {
              "name": "{{namespaceMemoryLimit_name}}",
              "results": {
                "aggregation_info": {
                  "sum": {{namespaceMemoryLimit_sum}},
                  "format": "{{namespaceMemoryLimit_format}}"
                }
              }
            },
            {
              "name": "{{namespaceMemoryUsage_name}}",
              "results": {
                "aggregation_info": {
                  "min": {{namespaceMemoryUsage_min}},
                  "max": {{namespaceMemoryUsage_max}},
                  "avg": {{namespaceMemoryUsage_avg}},
                  "format": "{{namespaceMemoryUsage_format}}"
                }
              }
            },
            {
              "name": "{{namespaceMemoryRSS_name}}",
              "results": {
                "aggregation_info": {
                  "min": {{namespaceMemoryRSS_min}},
                  "max": {{namespaceMemoryRSS_max}},
                  "avg": {{namespaceMemoryRSS_avg}},
                  "format": "{{namespaceMemoryRSS_format}}"
                }
              }
            },
            {
              "name": "{{namespaceTotalPods_name}}",
              "results": {
                "aggregation_info": {
                  "sum": {{namespaceTotalPods_sum}}
                }
              }
            },
            {
              "name": "{{namespaceRunningPods_name}}",
              "results": {
                "aggregation_info": {
                  "sum": {{namespaceRunningPods_sum}}
                }
              }
            }
          ]
        }
      }
    ]
  }
]
